{
  "version": "0.8.4",
  "packOptions": {
    "owners": [ "Shay Rojansky" ],
    "authors": [ "Shay Rojansky" ],
    "description": "Uses Roslyn to rewrite synchronous C# methods into async counterparts",
    "repository": {
      "type": "git",
      "url": "git://github.com/roji/AsyncRewriter"
    }
  },
  "buildOptions": {
    "includeFiles": "AsyncRewriterHelpers.cs"
  },
  "dependencies": {
    "Microsoft.CodeAnalysis.CSharp": "1.3.0-*"
